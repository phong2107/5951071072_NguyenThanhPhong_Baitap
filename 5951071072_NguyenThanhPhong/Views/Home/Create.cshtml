
@{
    ViewData["Title"] = "Create";
}

@using _5951071072_NguyenThanhPhong.Models
@model Employee

<form asp-action="Create" class="form-horizontal">

    <div class="form-group">
        <label class="custom-control-label">Name</label>
        <input asp-for="Emp_name" class="form-control" />

    </div>
    <div class="form-group">
        <label class="custom-control-label">City</label>
        <input asp-for="City" class="form-control" />

    </div>

    <div class="form-group">
        <label class="custom-control-label">State</label>
        <input asp-for="State" class="form-control" />

    </div>

    <div class="form-group">
        <label class="custom-control-label">Country</label>
        <input asp-for="Country" class="form-control" />

    </div>

    <div class="form-group">
        <label class="custom-control-label">Department</label>
        <input asp-for="Department" class="form-control" />

    </div>
    <div class="form-group">
        <input type="submit" class="btn btn-primary" value="submet"/>
    </div>



</form>

